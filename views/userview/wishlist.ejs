    <% if(products.length > 0) {%>

    <main class="main">
        <div class="page-header breadcrumb-wrap">
            <div class="container">
                <div class="breadcrumb">
                    <a href="index.html" rel="nofollow">Home</a>
                    <span></span> Shop
                    <span></span> Wishlist
                </div>
            </div>
        </div>
        <section class="mt-10 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <table class="table shopping-summery text-center">
                                <thead>
                                    <tr class="main-heading">
                                        <th scope="col" colspan="2">Product</th>
                                        <th scope="col">Price</th>
                                        <th scope="col">Stock Status</th>
                                        <th scope="col">Action</th>
                                        <th scope="col">Remove</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for (var i =0; i < products.length;  i++ ) { %>
                                    <tr>
                                        <td class="image"><img src="<%=products[i].product.image[0]%>" alt="#"></td>
                                        <td class="product-des">
                                            <h5 class="product-name"><a href="/product-details?id=<%=products[i].product._id%>"><%=products[i].product.name%></a></h5>
                                            <p class="font-xs"><%=products[i].product.description%>
                                            </p>
                                        </td>
                                        <td class="price" data-title="Price"><span><%=products[i].product.price%></span></td>



                                        <% if (products[i].product.stock == 0) {%>
                                        <td class="text-center" data-title="Stock">
                                            <span class="text-danger font-weight-bold">Out of stock</span>
                                        </td>
                                        <td>
                                            <a class="btn btn-sm btn-secondary text-light" onclick="outOfStock(); event.preventDefault()">Add to cart</a>
                                        </td>
                                            <% } else { %>
                                                <td class="text-center" data-title="Stock">
                                            <span class="text-danger font-weight-bold"><%=products[i].product.stock%></span>
                                        </td>
                                        <td class="" data-title="Cart">
                                            <a class="btn btn-sm btn-secondary"  href="/add-to-cart/<%=products[i].product._id%>"><i class=" mr-5"></i>Add to cart</a>
                                        </td>
                                            <% }%>
                                        <td class="action" data-title="Remove"><a href="/remove-wishlist/<%=products[i].product._id%>" class="text-danger">Remove</a></td>
                                    </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>
    
